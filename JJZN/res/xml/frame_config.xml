
<!-- 框架的配置文件 -->
<frame_config>

    <!-- 网络框架的HOST地址 -->
     <!-- <host value="http://gamma.yqby.jc-yt.com:8988/" />  -->
     <host value="http://yqby.jc-yt.com/" />

    <!-- 框架里的debug是否开启 -->
    <debug value="false" />

    <!-- 是否保存LOG到日志文件 -->
    <is_save_log value="true" />

    <!-- 系统日志存放的目录 -->
    <log_file_path value="/sdcard/log.txt" />
    
    <!-- 网络请求类型，使用并发还是使用单线程队列 1表示单线程队列，2表示使用线程池并发 -->
    <net_request_type value="1"/>

    <!--
         默认框架返回的数据类型 目前只支持4种
    	1:二进制数组
    	2:纯文本
    	3:解析成android内置的jsonObject对象
    	4:统一解析成MAP对象，当解析成Map对象时候
    		下面的data_type用来配置创建创建什么类型的解析器
    -->
    <data_response_type value="3" />

    <!-- 默认创建解析器的类型，该值只有当 -->
    <data_type value="json" />

</frame_config>